<!doctype html>
<html lang="zxx">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title> @ViewBag.Title - @AppService.AppName </title>
    <link rel="shortcut icon" type="image/x-icon" href="~/img/logoicon.png" .ico">


    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    @Html.Partial("~/Views/Shared/_PartialLinkCss.cshtml")

    @Html.Partial("~/Views/Shared/_PartialLinkJs.cshtml")

</head>
<body class="bg-light">

    <header>
        <!-- Header Start -->
        <div class="header-area">
            <div class="container">
                <div class="row text-center">
                    <div class="col-xl-12">
                        <!-- Logo -->
                        <div class="logo m-5">
                            <a href=@Url.Action("Index","Home")><img src="~/img/logo.png" alt=""></a>
                        </div>

                        @*<div class="hero-cap text-center">
                            </div>*@

                    </div>
                </div>
            </div>
        </div>
        <!-- Header End -->
    </header>

    <main>

        @RenderBody()
        

    </main>

    <footer>
        <!-- Footer Start-->
        <div class="footer-area ">
            <div class="container">



                <hr>
                <!-- Footer bottom -->
                <div class="row align-items-center">
                    
                    <div class="col-12 col-md-7">
                        <div class="footer-copy-right">
                            <p>
                                <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                                Copyright &copy; @DateTime.Today.Year All rights reserved | This template is made with  <a href=# target="_blank">@AppService.AppName</a>
                                <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                            </p>
                        </div>
                    </div>
                    <div class=" col-12 col-md-5">
                        <div class="footer-copy-right f-right">
                            <!-- social -->
                            <div class="footer-social">
                                <a href="#"><i class="fab fa-twitter"></i></a>
                                <a href="https://www.facebook.com/sai4ull"><i class="fab fa-facebook-f"></i></a>
                                <a href="#"><i class="fab fa-behance"></i></a>
                                <a href="#"><i class="fas fa-globe"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Footer End-->
    </footer>



</body>

</html>